; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="DJam24"
run/main_scene="res://ui/menu.tscn"
config/features=PackedStringArray("4.3", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Global="*res://autoload/global.gd"

[display]

window/size/window_width_override=400
window/size/window_height_override=300
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[navigation]

2d/default_cell_size=10.0

[physics]

common/physics_ticks_per_second=120
2d/physics_engine="GodotPhysics2D"
2d/default_gravity=0.0

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
environment/defaults/default_clear_color=Color(0.0627451, 0.0980392, 0.494118, 1)
